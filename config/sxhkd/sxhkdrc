alt + shift + Return
	uxterm

alt + shift + q
	bspc quit

alt + {j,k}
	bspc node -f {next,prev}.local

alt + {h,l}
	{bspc node -z right -20 0 || bspc node -z left -20 0,bspc node -z right 20 0 || bspc node -z left 20 0}

alt + Return
	bspc node -s biggest

alt + shift + c
	bspc node -c

alt + shift + {1-9}
	bspc desktop -f {1-9}.local
